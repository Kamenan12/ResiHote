{
  "expo": {
    "name": "ResiHote",
    "slug": "resihote",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.test.test"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.test.test"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "onesignal-expo-plugin",
        {
          "mode": "development"
        }
      ],
      [
        "./app.plugin",
        {
          "ios": {
            "CodePushDeploymentKey": ""
          },
          "android": {
            "CodePushDeploymentKey": "fvrXWaqLkljT0PuHHuVtuVW7mOYQHi-Lp6CXO"
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "build": {
          "experimental": {
            "ios": {
              "appExtensions": [
                {
                  "targetName": "OneSignalNotificationServiceExtension",
                  "bundleIdentifier": "com.test.test.OneSignalNotificationServiceExtension",
                  "entitlements": {
                    "com.apple.security.application-groups": [
                      "group.com.test.test.onesignal"
                    ]
                  }
                }
              ]
            }
          }
        },
        "projectId": "e166eb03-0a41-4e6f-89b5-3922fae67ceb"
      }
    },
    "owner": "kamenan12"
  }
}
